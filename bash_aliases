# bash_aliases, must be in ~/.bash_aliases

cls=clear
cp='cp -i'
dd='dd status=progress'
e=vim
g='| grep'
ga='git add'
gb='git branch'
gc='git commit'
gck='git checkout'
gcl='git clone'
gh='git push'
gl='git pull'
glg='git log --graph'
grep='grep --color=auto'
gs='git status'
gtfo=poweroff
la='ls -lah'
ll='ls -lh'
ls='ls --color'
mv='mv -i'
please=sudo
weechat=weechat-curses
which-command=whence
